<?xml version="1.0" encoding="UTF-8"?>
<highlighters>
	<highlighter type="string">
		<string>"</string>
		<escape>\</escape>
	</highlighter>
	<highlighter type="string">
		<string>'</string>
	</highlighter>
	<highlighter type="oneline-comment">//</highlighter>
	<highlighter type="hexnumber">
		<prefix>0x</prefix>
		<ignoreCase />
	</highlighter>
	<highlighter type="number">
		<point>.</point>
		<exponent>e</exponent>
		<suffix>f</suffix>
		<suffix>d</suffix>
		<suffix>l</suffix>
		<ignoreCase />
	</highlighter>
	<highlighter type="word">
		<word>gremlin&gt;</word>
		<style>repl-prompt</style>
	</highlighter>
	<highlighter type="regex">
		<pattern>==&gt;</pattern>
		<style>repl-result</style>
	</highlighter>
        <highlighter type="regex">
	        <pattern>(E|V||both|bothE|bothV|cap|gather|id|in|inE|inV|key|label|linkBoth|linkIn|LinkOut|map|memoize|order|orderMap|out|outE|outV|path|scatter|select|shuffle|transform|and|back|dedup|except|filter|has|hasNot|interval|or|random|retain|simplePath|aggregate|as|groupBy|groupCount|optional|sideEffect|store|table|tree|copySplit|exhaustMerge|fairMerge|ifThenElse|loop|addEdge|addVertex|addVertexWithLabel|removeEdge|removeVertex|fill|iterate|next)(\(| |\n|$)</pattern>
                <style>gremlin-func</style>
        </highlighter>
        <highlighter type="regex">
	        <pattern>\)</pattern>
                <style>gremlin-func</style>
        </highlighter>
</highlighters>
